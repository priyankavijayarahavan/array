#include <stdio.h>
void main ()
{
int num[50];
 int i, j, b, n;
 printf("Enter the value of N\n");
 scanf("%d", &n);
    printf("Enter the numbers \n");
for (i = 0; i < n; ++i)
scanf("%d", &num[i]);
 for (i = 0; i < n; ++i)
  {
for (j = i + 1; j < n; ++j)

 {

  if (num[i] < num[j])

   {

   b = num[i];

  num[i] = num[j];
   num[j] = b;

   }

   }

    }
 printf("The numbers arranged in decending\n");
 for (i = 0; i < n; ++i)
 {
  printf("%d\n", num[i]);
 }
}
